import static java.lang.System.*;

public class Triples
{
   private int number;

	public Triples()
	{
		this(0);
		number = 0;
	}

	public Triples(int num)
	{
		setNum(num);
	}

	public void setNum(int num)
	{
		number = num;
	}
	
	private int greatestCommonFactor(int a, int b, int c)
	{
		int max = 0;
		for(a = 1; a <= 110; a++){
			for(b = 1; b <= 110; b++){
				for(c = 1; c <= 110; c++){
					if (((double)a*(double)a) + ((double)b*(double)b) == ((double)c*(double)c)){
						if((a % 2 == 1)){
							//(b % 2) = 0;
						}
					}
					
				}
			}
		}



		return 1;
	}

	public String toString()
	{
		String output="";






		return output+"\n";
	}
}